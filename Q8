def count_consonants(s):
    consonants = "bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ"
    count = 0

    for char in s:
        if char in consonants:
            count += 1

    return count

# Test cases
print(count_consonants("abc de"))              # Output: 3
print(count_consonants("geeksforgeeks portal"))  # Output: 12
